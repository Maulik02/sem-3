#include<iostream.h>
#include<conio.h>
class rectangle
{
	private:
		int lenght;
		int width;
	public:
		void getdata(int a,int b)
		{
			lenght=a;
			width=b;
		}
		int area()
		{
			return(lenght*width);
		}
};
